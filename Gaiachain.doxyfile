DOXYFILE_ENCODING = UTF-8

PROJECT_NAME = Gaiachain
PROJECT_NUMBER = 1.0
PROJECT_BRIEF = "TEMPLATE doxygen file - use it in your project"
PROJECT_LOGO = Gaiachain.png

OUTPUT_DIRECTORY = doc/

INPUT = bumpVersion.sh license-Qt.txt main.cpp AUTHORS.md README.md \
